namespace a:
import ART.art_Sink_txt;

    type T1 : A(int x, float y) | B end

    actor b() T1 i ==> T1 o, int p:
    
        initialize ==> o:[oo]
        var
            T1 oo
        do
            oo:=T1::A(0,0.0);
        end

        bool done := false;

        loop: action i:[a] ==> o:[oo], p:[pp]
        guard not done
        var
            T1 oo,
            int pp
        do
          oo := case a of          
            A(iix, iiy) guard iix < 10  : T1::A(iix+1,0) end               	 
            A(iix, iiy) guard iix >= 10 : T1::B() end
            B() : a end
          end;
          
	 	  done := case a of 
		    A(_,_) : false end
            B() : true end
          end;  
          
          pp := case a of 
            A(iix, _) guard iix < 10 : iix+1 end
			A(iix, _) guard iix >= 10: -1 end
            B() : -2 end
          end;  
        end                

    end
    
    network top() ==>:
    entities
        f = b();
        print=art_Sink_txt(fileName = "./output/caseTestExpr.txt");
        
    structure
        f.o --> f.i;
        f.p --> print.In;
    end

end
